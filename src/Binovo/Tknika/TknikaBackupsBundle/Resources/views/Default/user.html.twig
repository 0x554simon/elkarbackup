{% extends 'BinovoTknikaTknikaBackupsBundle:Default:base.html.twig' %}
{% trans_default_domain 'BinovoTknikaBackups' %}

{% block body %}

<form class="form-horizontal" action="{{ path('saveUser', {id: form.vars.value.id}) }}" method="post" {{ form_enctype(form) }}>
    <legend>{% trans %}Edit User{% endtrans %}</legend>
{% form_theme form 'BinovoTknikaTknikaBackupsBundle:Default:fields.html.twig' %}
    <div>
        {{ form_widget(form) }}
    </div>
    <input class="btn" type="submit" value="{% trans %}Save{% endtrans %}"/>
</form>

{% endblock %}
