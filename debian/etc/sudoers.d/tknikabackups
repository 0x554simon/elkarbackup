Cmnd_Alias TKNIKABACKUPS_SCRIPTS=/usr/share/tknikabackups/uploaded/*
Defaults!TKNIKABACKUPS_SCRIPTS env_keep += TKNIKABACKUPS_LEVEL, env_keep += TKNIKABACKUPS_EVENT, env_keep += TKNIKABACKUPS_URL, env_keep += TKNIKABACKUPS_ID, env_keep += TKNIKABACKUPS_PATH, env_keep += TKNIKABACKUPS_STATUS
tknikabackupsuser ALL = NOPASSWD: TKNIKABACKUPS_SCRIPTS
