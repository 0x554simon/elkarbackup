Cmnd_Alias ELKARBACKUP_SCRIPTS=/usr/share/elkarbackup/uploaded/*
Defaults!ELKARBACKUP_SCRIPTS env_keep += ELKARBACKUP_LEVEL, env_keep += ELKARBACKUP_EVENT, env_keep += ELKARBACKUP_URL, env_keep += ELKARBACKUP_ID, env_keep += ELKARBACKUP_PATH, env_keep += ELKARBACKUP_STATUS
elkarbackup ALL = NOPASSWD: ELKARBACKUP_SCRIPTS
